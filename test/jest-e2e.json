{
    "moduleDirectories": ["node_modules", "<rootDir>/../"],
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "setupFilesAfterEnv": ["./setup.ts"],
    "testEnvironment": "node"
  }